vector<int> leaders(int a[], int n){
        vector<int>ans;
         int max=INT_MIN;
         for(int i=n-1; i>=0; i--) {
             if(a[i]>=max){ 
             ans.push_back(a[i]);
             max=a[i];
             }
         }
         reverse(ans.begin(),ans.end());
         return ans;
        // here we are iterating the elements from the back and we are getting it bu taking the INT_MIN and comparing with that we are getting the element 
        
