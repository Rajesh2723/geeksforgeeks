#include <iostream>
#include<bits/stdc++.h>
using namespace std;

void prime(int n){
    int count=0;
    for(int i=2;i<=n;i++){
        if(n%i==0){
            count+=1;
        }
    }
        if(count==1){
            cout<<" prime number"<<endl;
        }else{
            cout<<" not a prime number "<<endl;
        }
    
}
int main(){
    int n;
    cin>>n;
    prime(n);
    return 0;
}
